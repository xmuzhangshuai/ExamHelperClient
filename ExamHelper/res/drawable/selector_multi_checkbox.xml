<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:drawable="@drawable/multi_check_off" android:state_checked="false" android:state_enabled="true"/>
    <item android:drawable="@drawable/multi_check_on" android:state_checked="true" android:state_enabled="true"/>
    <item android:drawable="@drawable/multi_check_off_disable" android:state_checked="false" android:state_enabled="false"/>
